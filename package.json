    "@babel/preset-react": "^7.0.0",
    "archiver": "^3.1.1",
    "autoprefixer": "^9.6.1",
    "babel-loader": "^8.0.6",
    "body-parser": "^1.19.0",
    "browser-sync": "^2.26.7",
    "chalk": "^2.4.2",
    "chokidar": "^3.0.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^5.0.4",
    "core-js": "^3.2.1",
    "cosmiconfig": "^5.2.1",
    "css-loader": "^3.2.0",
    "cssnano": "^4.1.10",
