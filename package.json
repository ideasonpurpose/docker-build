  "contributors": [
    "Joe Maller <joe@ideasonpurpose.com>"
  ],
  "main": "index.js",
  "scripts": {
    "analyze": "WEBPACK_BUNDLE_ANALYZER=true webpack",
    "postanalyze": "node explore.js",
    "explore": "source-map-explorer",
    "build": "NODE_ENV=production webpack --progress --color --display-error-details",
    "postbuild": "echo 'npm run zip' && npm run build:preview",
    "build:preview": "node browsersync.js ",
    "env": "env",
    "devserver": "npm run start",
    "start": "webpack-dev-server",
    "webpack": "webpack",
    "zip": "node zip.js"
  },
  "dependencies": {
    "@babel/core": "^7.5.5",
    "@babel/plugin-syntax-dynamic-import": "^7.2.0",
